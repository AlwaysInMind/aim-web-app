{
  "name": "always-in-mind",
  "version": "0.1.0",
  "private": true,
  "zengines": {
    "node": "8.11.3"
  },
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "auth0-js": "9.7.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-modal": "3.6.1",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "react-scripts": "^2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "postbuild": "copy package.now.json build\\package.json && del /q build\\static\\js\\*.map",
    "deploy": "yarn run build && now --public deploy build/ && now alias alwaysinmind.org",
    "pause": "node -e \"((time) => new Promise((resolve) => setTimeout(resolve, time)))(process.argv[1]).then(() => process.exit(0))\" 30000"
  },
  "now": {
    "name": "alwaysinmind"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
